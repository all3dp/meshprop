{
  "name": "meshprop",
  "version": "2.4.0",
  "description": "Get 3D mesh properties",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm ls",
    "install": "node-pre-gyp install --fallback-to-build",
    "build": "node-gyp configure && node-pre-gyp clean && node-pre-gyp build",
    "test": "mocha tests/*.js",
    "release": "node-pre-gyp package && node-pre-gyp testpackage && node-pre-gyp unpublish && node-pre-gyp publish"
  },
  "author": "Jonathan HÃ¤berle <jonathan.haeberle@gmail.com> (https://github.com/dreampulse/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/all3dp/meshprop"
  },
  "keywords": [
    "3d",
    "mesh",
    "stl"
  ],
  "dependencies": {
    "node-pre-gyp": "^0.10.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "aws-sdk": "^2.245.1",
    "mocha": "^5.2.0"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "gypfile": true,
  "binary": {
    "module_name": "meshprop",
    "module_path": "./build/{configuration}/",
    "remote_path": "./{module_name}/v{version}/{configuration}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://meshprop.s3-eu-west-1.amazonaws.com"
  }
}
